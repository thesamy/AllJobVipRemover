{
  "name": "AllJobsVipReomver",
  "version": "0.1",
  "description": "Remove the vip section from alljobs search result pages",
  "permissions": [
    "http://www.alljobs.com/*"
  ],
  "content_scripts": [
    {
	   "matches": ["http://www.alljobs.co.il/*"],
      "js": ["src/ContentPage/ContentScript.js"]
    }
  ],
  "browser_action": {
  },
  "icons": { 
          "16" : "icons/icon16.png",
          "48" : "icons/icon48.png",
          "128": "icons/icon128.png" },
  "manifest_version": 2
}
